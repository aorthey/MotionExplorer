#include <ompl/multilevel/datastructures/explorer/PlannerMultiLevelExplorer.h>

ompl::multilevel::PlannerMultiLevelExplorer::PlannerMultiLevelExplorer(
    std::vector<base::SpaceInformationPtr> &siVec, std::string type)
  : BaseT(siVec, type)
{
    localMinimaTree_ = std::make_shared<LocalMinimaTree>(siVec);
}
MinimaTreePtr& PlannerMultiLevelExplorer::getLocalMinimaTree()
{
    return localMinimaTree_;
}

